{
  "private": true,
  "workspaces": [
    "starters/*",
    "themes/*"
  ],
  "name": "gatsby-theme-blank-root",
  "devDependencies": {
    "@testing-library/cypress": "^6.0.0",
    "cypress": "^5.0.0",
    "gatsby-cypress": "^0.4.10",
    "lerna": "^3.22.1",
    "start-server-and-test": "^1.11.3"
  },
  "scripts": {
    "develop:blank": "yarn workspace gatsby-starter-blank develop",
    "build:blank": "yarn workspace gatsby-starter-blank build",
